# =====================================================
# VALUE FDORGMT
# =====================================================

def FDORGMT(months):
    if months <= 1:
        return "12"
    elif months <= 2:
        return "13"
    elif months <= 3:
        return "14"
    elif months <= 6:
        return "15"
    elif months <= 9:
        return "16"
    elif months <= 12:
        return "17"
    elif months <= 15:
        return "21"
    elif months <= 18:
        return "22"
    elif months <= 24:
        return "23"
    elif months <= 36:
        return "24"
    elif months <= 48:
        return "25"
    elif months <= 60:
        return "26"
    else:
        return "30"


# =====================================================
# VALUE FDRMMT
# =====================================================

def FDRMMT(months):
    if months <= 0:
        return "51"
    elif months <= 1:
        return "52"
    elif months <= 2:
        return "53"
    elif months <= 3:
        return "54"
    elif months <= 6:
        return "55"
    elif months <= 9:
        return "56"
    elif months <= 12:
        return "57"
    elif months <= 24:
        return "61"
    elif months <= 36:
        return "62"
    elif months <= 48:
        return "63"
    elif months <= 60:
        return "64"
    else:
        return "70"


# =====================================================
# VALUE RMFDORGMT
# =====================================================

_RMFDORGMT = {
    272: "12", 284: "12",
    273: "13", 285: "13",
    274: "14", 286: "14",
    275: "15", 276: "15", 277: "15",
    287: "15", 288: "15", 289: "15",
    278: "16", 279: "16", 280: "16",
    290: "16", 291: "16", 292: "16",
    281: "17", 282: "17", 283: "17",
    293: "17", 294: "17", 295: "17",
}

def RMFDORGMT(code):
    """
    SAS: put(code, RMFDORGMT.)
    """
    return _RMFDORGMT.get(code)
