"""
Direct conversion of SAS PROC FORMAT
VALUE DDCUSTCD
VALUE STATECD
VALUE FDORGMT
VALUE FDRMMT
VALUE RMFDORGMT
Original names preserved
"""

# =====================================================
# VALUE DDCUSTCD
# =====================================================

_DDCUSTCD = {
    1: "01", 2: "30", 3: "30", 4: "04", 5: "05", 6: "06",
    10: "30", 11: "30", 12: "12", 13: "13", 15: "62",
    17: "17", 20: "30", 30: "30", 31: "31", 32: "32",
    33: "33", 34: "34", 35: "35", 36: "06", 37: "37",
    38: "38", 39: "39", 40: "40", 41: "41", 42: "42",
    43: "43", 44: "44", 45: "45", 46: "46", 47: "47",
    48: "48", 49: "49", 50: "78", 51: "51", 52: "52",
    53: "53", 54: "54", 57: "57", 59: "59", 60: "62",
    61: "61", 62: "62", 63: "63", 64: "64",
    65: "44", 66: "41", 67: "44", 68: "48", 69: "52",
    70: "71", 71: "71", 72: "72", 73: "73", 74: "74",
    75: "75", 76: "78", 77: "77", 78: "78", 79: "79",
    80: "86", 81: "86", 82: "86", 83: "86", 84: "86",
    85: "86", 86: "86", 87: "87", 88: "88", 89: "89",
    90: "90", 91: "91", 92: "92", 95: "95", 96: "96",
    98: "98", 99: "99",
}

def DDCUSTCD(code):
    """
    SAS: put(code, DDCUSTCD.)
    OTHER = '79'
    """
    return _DDCUSTCD.get(code, "79")


